    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "number-input": "fooloomanzoo/number-input#2",
    "text-input": "fooloomanzoo/text-input#2",
    "datetime-input": "fooloomanzoo/datetime-input#2",
    "datetime-picker": "fooloomanzoo/datetime-picker#2",
    "color-input": "fooloomanzoo/color-input#2",
    "color-picker": "fooloomanzoo/color-picker#2"
  },
  "devDependencies": {
    "webcomponentsjs": "webcomponents/webcomponentsjs",
