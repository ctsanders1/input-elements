{
  "name": "@fooloomanzoo/input-elements",
  "version": "3.0.0",
  "description": "customElements for enhancing or replacing native inputs",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build && git add . && git commit -m \"publish new version\" && npm version patch && git push",
    "build": "git pull && npm update && npm run analyze",
    "start": "npm run analyze && polymer serve -o",
    "analyze": "polymer analyze > analysis.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooloomanzoo/input-elements.git"
  },
  "keywords": [
    "input",
    "picker",
    "polymer"
  ],
  "author": "fooloomanzoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fooloomanzoo/input-elements/issues"
  },
  "dependencies": {
    "@fooloomanzoo/color-input": "^3",
    "@fooloomanzoo/color-picker": "^3",
    "@fooloomanzoo/datetime-input": "^3",
    "@fooloomanzoo/datetime-picker": "^3.0.6",
    "@fooloomanzoo/number-input": "^3",
    "@fooloomanzoo/text-input": "^3",
    "@polymer/polymer": "^3"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3",
    "@polymer/iron-flex-layout": "^3",
    "@polymer/iron-form": "^3",
    "@polymer/iron-test-helpers": "^3",
    "@webcomponents/webcomponentsjs": "^2"
  },
  "homepage": "https://github.com/fooloomanzoo/input-elements#readme"
}
